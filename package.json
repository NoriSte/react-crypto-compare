    "@svgr/rollup": "^2.4.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^23.1.5",
    "@types/node": "^12.0.2",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^7.0.0-beta.41",
