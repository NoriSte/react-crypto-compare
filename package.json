{
  "name": "react-crypto-compare",
  "version": "1.3.0",
  "description": "A simple cryptocurrencies converter component based on https://min-api.cryptocompare.com",
  "author": "NoriSte &lt;nori.ste.magni@gmail.com&gt;",
  "license": "MIT",
  "repository": "https://github.com/NoriSte/react-crypto-compare",
  "keywords": [
    "react",
    "react-hooks",
    "cypress",
    "cryptocurrencies",
    "typescript"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "yarn build && cypress run",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "cy:open": "cypress open",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@cypress/webpack-preprocessor": "4.1.0",
    "@svgr/rollup": "4.3.0",
    "@types/axios": "0.14.0",
    "@types/classnames": "2.2.8",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.8",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-runtime": "6.26.0",
    "cross-env": "5.2.0",
    "cypress": "3.3.1",
    "cypress-react-unit-test": "2.4.1",
    "gh-pages": "2.0.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts-ts": "3.1.0",
    "rollup": "1.15.6",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.0.3",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-typescript2": "0.21.2",
    "rollup-plugin-url": "2.2.2",
    "ts-loader": "6.0.2",
    "typescript": "3.5.2",
    "typescript-is-type": "1.0.1",
    "webpack": "4.35.0"
  },
  "files": [
    "dist"
  ]
